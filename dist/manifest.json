{
  "manifest_version": 3,
  "name": "FPTU Move Out Class Tool",
  "description": "Tiện ích hỗ trợ sinh viên FPTU xem lịch, chuyển lớp nhanh trên FAP",
  "version": "1.3.8.2",
  "content_scripts": [
    {
      "js": ["index.js"],
      "css": ["index.css"],
      "matches": ["https://fap.fpt.edu.vn/*"]
    }
  ],
  "permissions": ["storage"],
  "host_permissions": ["https://fap.fpt.edu.vn/*"],
  "action": {
    "default_icon": "image.png"
  },
  "icons": {
    "16": "image.png",
    "32": "image.png",
    "48": "image.png",
    "128": "image.png"
  },
  "applications": {
  "gecko": {
    "id": "fptu-move-out-class-tool@example.com",
    "strict_min_version": "109.0"
  }
}
}
